{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "framework": null,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/client/(.*)", "dest": "/client/index.html" },
    { "src": "/client", "dest": "/client/index.html" },
    { "src": "/dashboard/(.*)", "dest": "/dashboard/index.html" },
    { "src": "/dashboard", "dest": "/dashboard/index.html" }
  ]
}
